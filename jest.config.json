{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "components/**/*.{js,jsx}",
    "lib/**/*.{js,jsx}",
    "model/**/*.{js,jsx}",
    "app/**/*.{js,jsx}",
    "pages/**/*.{js,jsx}",
    "!**/*.test.{js,jsx}",
    "!**/*.spec.{js,jsx}",
    "!**/__tests__/**",
    "!**/__mocks__/**",
    "!**/node_modules/**",
    "!**/coverage/**",
    "!**/build/**",
    "!**/dist/**",
    "!**/*.config.{js,jsx}",
    "!**/jest.setup.js",
    "!pages/_app.js",
    "!pages/_document.js"
  ],
  "coverageReporters": [
    "text",
    "lcov",
    "json-summary"
  ],
  "setupFilesAfterEnv": ["./jest.setup.js"],
  "testMatch": [
    "**/__tests__/**/*.test.[jt]s?(x)"
  ],
  "testEnvironment": "jest-environment-jsdom",
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "modulePaths": [
    "<rootDir>"
  ],
  "moduleNameMapper": {
    "@/(.*)$": "<rootDir>/$1",
    "^/(.*)$": "<rootDir>/$1",
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest",
    "^.+\\.(css|sass|scss)$": "jest-css-modules-transform"
  }
}
